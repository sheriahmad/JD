/* 2ND & 3RD LEVEL: LEFT NAV */
#left_nav h2 {
	font-size: 10pt;
	text-transform: uppercase;
	margin: .5em 1em;
	/* padding: .5em 1em; */
	color: #666;
/*	border-bottom: 2px #d41421 solid; */
	}
#left_nav ul {
	/* width: 150px; */
	margin: 0;
	}
#left_nav ul li {
	list-style: none;
	margin: 0;
	line-height: 1em;
	}
#left_nav ul br {
	line-height: .5em;
	}

#left_nav ul li a {
	display: block;
	padding: .25em 0 .25em 1.5em;
	/* margin: .25em 0 .25em 1.5em; */
	/* padding-left:15px; */
	}
#left_nav ul li a:link {
/*	color: #d41421; */
	color: #000;
	text-decoration: none;
	}
#left_nav ul li a:visited {
/*	color: #d41421; */
	color: #000;
	text-decoration: none;
	}
#left_nav ul li a:hover {
	width: 100%;
	background-color: #d41421;
	color: #fff;
	text-decoration: none;
	}
#left_nav ul li a:active {
	width: 100%;
	background-color: #d41421;
	color: #fff;
	text-decoration: none;
	}

